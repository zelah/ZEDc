#include<stdio.h>
#include<stdlib.h>
#include<time.h>
#define tw(x) x=x+x
#define mu(x,y) x=y=x*y
#define sq(x) x=mu(x,x)
#define profile srand(time(NULL))
#define pro1 printf(" PROFILE:%d ",1/(rand()/(RAND_MAX/1000)))
#define pro0
#define pro pro0

typedef char ch;
typedef unsigned char uc;
typedef signed char sc;
typedef short sh;
typedef unsigned short us;
typedef signed short ss;
typedef int in;
typedef unsigned int ui;
typedef signed int si;
typedef long lo;
typedef unsigned long lu;
typedef signed long ls;
typedef long long ll;
typedef unsigned long long ul;
typedef signed long long sl;
typedef float fl;
typedef double db;
typedef long double ld;

int main()
{
  profile;
  db x=5.0;
  tw(x);
  sq(x);
  sq(x);
  pro;
  pro;
  printf("%f",x);
  return 0;
}
